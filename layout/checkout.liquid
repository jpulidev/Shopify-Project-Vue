  <script>
  // Retrieve Mixpanel ID
  mixpanel.identify('{{ attributes.mixpanel_id }}');
  
  mixpanel.people.set({
    "$name": "{{ customer.first_name }} {{ customer.last_name }}",
    "$email": "{{ customer.email }}",
    "last_updated": new Date()
  });
  
  mixpanel.people.track_charge("{{order.total_price | money_without_currency}}".replace(",",""));
  
  mixpanel.track("Checkout",
  {
     orderId: '{{checkout.order_id}}',
     customerName:'{% if customer %} {{ customer.first_name }}{% endif %}',
     orderDiscountCode: '{% for discount_application in checkout.discount_applications %} {{ discount_application.title }}{% endfor %}',     // Discount code used on order, or discount group name. Used to identify commissionable orders.
     orderDiscount: '{{discount_discount}}',
     orderShipping: '{{order.total_price | money_without_currency}}',
     orderSubtotal: '{{order.total_price | money_without_currency}}',     // Order subtotal. After discount is applied, before tax & shipping. Commission calculated from this.
     orderTax: '{{order.total_price | money_without_currency}}',
     orderCurrency: 'USD',
     orderTotal: '{{order.total_price | money_without_currency}}' });
  </script>
<!-- End Mixpanel --> 

    {% render 'checkout-scripts' %}
        <!-- Start of HubSpot Embed Code -->
<script type="text/javascript" id="hs-script-loader" async defer src="//js.hs-scripts.com/419889.js"></script>

<script>
    var hsUniqueKey = window.localStorage.getItem('hs_unique_user_id');
    if (!hsUniqueKey) {
        hsUniqueKey = Math.floor(Math.random() * 10000000);
        window.localStorage.setItem('hs_unique_user_id', hsUniqueKey);
    }
  
  
    var identifyObject = {
        id: hsUniqueKey
    };
  
    var userEmail = "{{ checkout.email }}";
    if (userEmail.length) {
      identifyObject.email = userEmail;
    }
                
                
    var _hsq = window._hsq = window._hsq || [];
    _hsq.push(["identify", identifyObject]);
</script>
<!-- End of HubSpot Embed Code -->
    
