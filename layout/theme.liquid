<body class="template-{{ request.page_type | handle }}">
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-W7LSMV6"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
  <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

  {%- if settings.enable_ajax -%}
    {% include 'cart-popup' %}
  {%- endif -%}

  {% section 'header' %}

  <div class="page-container drawer-page-content" id="PageContainer">

    {% if template == "product" %}
      {% section 'pdp-announcement-bar' %}
    {% endif %}

    <main class="main-content js-focus-hidden" id="MainContent" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% section 'footer-new' %}

    <div id="slideshow-info" class="visually-hidden" aria-hidden="true">
      {{- 'sections.slideshow.navigation_instructions' | t -}}
    </div>

  </div>

  <ul hidden>
    <li id="a11y-refresh-page-message">{{ 'general.accessibility.refresh_page' | t }}</li>
    <li id="a11y-selection-message">{{ 'general.accessibility.selection_help' | t }}</li>
  </ul>


  {% render 'modal-limit-dna' %}
  {% render 'modal-limit-one' %}

  
  {% if settings.restricted_states %}
  {% assign restricted_states = settings.restricted_states | split: "," %}
  <script>
    const RESTRICTED_STATES = [
      {% for state in restricted_states %}
        "{{ state }}" {% unless forloop.last %}, {% endunless %}
      {% endfor %}
    ]
  </script>
  {% endif %}

  <script>
    let currentCustomer = {%- if customer -%}'{{ customer.email }}'{%- else -%}''{%- endif -%};

    let customerTags = [
                        {%- if customer -%}
                          {%- for tag in customer.tags -%}
                            '{{ tag }}'{%- unless forloop.last -%}, {%- endunless -%}
                          {%- endfor -%}
                        {%- endif -%}
                      ]
  </script>

  {% assign removeStates = shop.metafields.globals.remove_states | split: "|" %}
  <script>
    const REMOVE_STATES = [
      {% for state in removeStates %}
        "{{ state }}"{% unless state.forloop.last %}, {% endunless%}
      {% endfor %}
    ];
  </script>
